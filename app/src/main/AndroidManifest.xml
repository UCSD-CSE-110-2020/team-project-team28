<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wwr">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.wwr.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.wwr.AskHeight_Activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.wwr.RoutesActivity" />
        <activity android:name="com.example.wwr.RouteDetail" />
        <activity android:name="com.example.wwr.RouteScreen" />
        <activity
            android:name="com.example.wwr.WalkScreenActivity"
            android:label="@string/title_activity_walk_screen"
            android:theme="@style/AppTheme.NoActionBar" />

        <service android:name="com.example.wwr.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>


        <activity
            android:name="com.example.wwr.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>